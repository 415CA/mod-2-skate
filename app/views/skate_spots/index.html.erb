<% @skate_spots.each do  |skate_spot| %>
  <%= link_to skate_spot.name, "skate_spot" %> | Rating <%= skate_spot.rating%>
  <br>
  Description: <%= skate_spot.description %>
  <br>
  <iframe src="<%= skate_spot.google_map %>" width="400" height="300" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe>
<% end %>
