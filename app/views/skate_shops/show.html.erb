  <h1> <%=@skate_shop.name %> </h1>
  Address: <%=@skate_shop.address %>
  <br>
  Borough: <%=@skate_shop.borough %>
  <br>
  Rating: <%= @skate_shop.rating%>
  <br>
  Description: <%= @skate_shop.description %>
  <br>
  Map:
  <br>
  <iframe src="<%= @skate_shop.google_map %>" width="400" height="300" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe>

   <hr>
   <h3>Reviews</h3>
<%@skate_shop.shop_reviews.each do |review|%>
 <div class="container">
 <b><%= link_to review.user.name, user_path(review)%>:</b>
        <%= review.title%>/
          <b>rating:</b><%= review.rating%><br>
    <%= review.content %></p>
    <% if review.user == current_user %>
     <%= button_to "delete", "/shop_reviews/#{review.id}", method: :delete %>
    <% end %>
     </div>
<%end%>
