<%= form_for @skate_shop do |f| %>
  <% if @skate_shop.errors.any? %>
    <div id="error_explanation">
      <h2>
        <%= pluralize(@skate_shop.errors.count, "error") %>
        prohibited this skate_shop from being saved:
      </h2>
  
      <ul>
      <% @skate_shop.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <br>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <br>
  <br>
  <%= f.label :borough %>
  <%= f.text_field :borough %>
  <br>
  <br>
  <%= f.label :description %>
  <%= f.text_field :description  %>
  <br>
  <br>
  <%= f.label :rating %>
  <%= f.text_field :description  %>
  <br>
  <br>
  <%= f.label 'Google Map Link' %>
  <%= f.text_field :google_map  %>
  <br>
  <br>
  <%= f.submit "Create Review" %>
<% end %>